<b><h1>
    @if (Context.User.IsInRole("A"))
    {
        <img style="width:50px;height:50px" src="~/assets/images/adminicon.png" />
    }
    else
    {
        <img style="width:50px;height:50px" src="~/assets/images/usericon.png" />
    }
    Bienvenido @Context.User.Identity.Name
</h1></b>
<hr/>
<h4><u>Ultimos datos registrados</u></h4>
<br/>
<br/>
<dl class="row">
    <dt class="col-sm-2">
        Total Partes:
    </dt>
    <dd class="col-sm-10">
        @ViewBag.TotalPartes
    </dd>
    <dt class="col-sm-2">
        Cerrados sin Fecha Resolución:
    </dt>
    <dd class="col-sm-10">
        @ViewBag.CerradosSF
    </dd>
    <dt class="col-sm-2">
        Cerrados:
    </dt>
    <dd class="col-sm-10">
        @ViewBag.Cerrados
    </dd>
    <dt class="col-sm-2">
        Entregados en gestión:
    </dt>
    <dd class="col-sm-10">
        @ViewBag.EntregadosEnGestion
    </dd>
    
</dl>